<%- include ('../partials/header');%>


<div class = "img" style="background-image: url(/coffee.png);height: 250px; width: 99%;" ></div>

       
        
        <h1 style="margin: 10px;"><%= day %> </h1>  
    <table style = "margin-top: 10px;margin : 10px ; border: 2px solid black;">
        <td>
            <tr>
                    <th><h3>User_id</h3></th>
                    <th><h3>Day</h3></th>
                    <th><h3>Start_At</h3></th>
                    <th><h3>End_At</h3></th>
            </tr>
            <% schedules.forEach(schedule=> { %>
            <tr>
                    <td><%= schedule.user_id %> </td> 

                    <td><%= schedule.day %> </td>

                    <td> <%= schedule.start_at %> </td>

                    <td><%= schedule.end_at %></td>
            </tr>
            <% }) %>
        </td>

    </table>
<br>
<br>    
<%- include ('../partials/footer');%>