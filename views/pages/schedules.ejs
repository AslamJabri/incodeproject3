<%- include ('../partials/header');%>

<div class = "img" style="background-image: url(./coffee.png);height: 150px;" >
    
        <h1 style="margin: 10px;"><%= day %> </h1>
       
</div>

<table style="color: black; margin-top: 10px;">
    <tr>
        <tr>
            <th><h3>User_id</h2></th>
            <th><h3>Day</h2></th>
            <th><h3>Start_At</h2></th>
            <th><h3>End_At</h2></th>
        </tr>
            <% schedules.forEach(schedules => { %>
        <tr>
          <td><%= schedules.user_id %> </td>
       
       
          <td><%=  schedules.day %> </td>
        
       
          <td><%= schedules.start_at %> </td>
        
       
          <td ><%= schedules.end_at %> </td>
        </tr>
        <% }); %> 
    </tr>
</table>

<%- include ('../partials/footer');%>